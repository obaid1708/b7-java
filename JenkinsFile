pipeline {
  agent any
    stages {
    stage('clean'){
        steps {
            sh 'mvn clean'
        }
    }
    stage ('compile'){
        steps {
            sh 'mvn compile'
        }
    }
    stage ('Test'){
        steps{
            sh 'mvn test'
        }
    }
    stage ('build'){
        steps{
            sh 'mvn install'
        }
    }
stage ('postbuild'){
steps{
sh 'echo "hello" >> /prac/maven/file1.txt '
    
}
}
}
}
